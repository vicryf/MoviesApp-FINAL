<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vicryf1504373.utsmobpro">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.vicryf1504373.utsmobpro.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vicryf1504373.utsmobpro.DetailActivity"
            android:label="Movies"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName="com.vicryf1504373.utsmobpro.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vicryf1504373.utsmobpro.MainActivity" />
        </activity>
        <activity android:name="com.vicryf1504373.utsmobpro.SettingsActivity"
            android:label="Settings" />
    </application>

</manifest>